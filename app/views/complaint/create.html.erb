<h1>Complaint#create</h1>
<%= form_tag "/complaint/create" do %>
  <%= label_tag :title, 'Title: ' %>
  <%= text_field_tag :title %><br />
  <%= label_tag :email, 'Your Email: ' %>
  <%= email_field_tag :email %><br />
  <%= label_tag :description, 'Description: ' %>
  <%= text_area_tag :description, nil, :rows => "10", :cols => "25"%><br />
  <%= hidden_field_tag :user, params[:user] %>
  <%= hidden_field_tag :ip_address %>
	<%= submit_tag 'Submit' %>
<% end %>